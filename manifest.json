{
  "manifest_version": 2,
  "name": "w//ordfinder-wiktionary",
  "description": "Search words from English Wiktionary with just a few clicks.",
  "version": "1.0.0",
  "author": "Jani Nevaranta",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_title": "W//OrdFinder",
    "default_popup": "popup/index.html"
  },
  "permissions": [
    "menus",
    "activeTab",
    "storage"
  ]
}